{
  "extends": [
    "airbnb",
    "plugin:meteor/recommended"
  ],
  "installedESLint": true,
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "meteor"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "import/resolver": "meteor"
  },
  "rules": {
    "linebreak-style": "off",
    "eol-last": "off"
  }
}